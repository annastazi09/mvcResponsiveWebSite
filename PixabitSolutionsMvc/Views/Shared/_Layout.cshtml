@using System.Web.Mvc;

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Pixabit solutions GmbH</title>
    <link rel="icon" href="@Url.Content("~/Content/img/favicon.ico")" type="image/x-ico; charset=binary">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/preload.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/bootstrap.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/font-awesome.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/animate.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.bxslider.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/width-boxed.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/style-blue.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/width-full.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/buttons.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/Site.css")" />

    <script src="@Url.Content("~/Scripts/jquery-1.10.2.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.js")"></script>
</head>

<body>
    @Html.Partial("_Header")
    @Html.Partial("_MainMenu")
   
    @RenderBody()

    @Html.Partial("_Footer")
</body>
</html>
